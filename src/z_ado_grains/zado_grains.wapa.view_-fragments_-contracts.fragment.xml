<c:FragmentDefinition xmlns="sap.m" xmlns:c="sap.ui.core" xmlns:f="sap.f" xmlns:smartFilterBar="sap.ui.comp.smartfilterbar" xmlns:smartTable="sap.ui.comp.smarttable">                                                                                         
    <f:DynamicPage>                                                                                                                                                                                                                                            
        <f:title>                                                                                                                                                                                                                                              
            <f:DynamicPageTitle backgroundDesign="Translucent">                                                                                                                                                                                                
                <f:heading>                                                                                                                                                                                                                                    
                    <Text text="Gestão de Contratos Agrícolas" class="headertitle" />                                                                                                                                                                          
                </f:heading>                                                                                                                                                                                                                                   
            </f:DynamicPageTitle>                                                                                                                                                                                                                              
        </f:title>                                                                                                                                                                                                                                             
        <f:header>                                                                                                                                                                                                                                             
            <f:DynamicPageHeader backgroundDesign="Translucent">                                                                                                                                                                                               
                <f:content>                                                                                                                                                                                                                                    
                    <VBox>                                                                                                                                                                                                                                     
                        <smartFilterBar:SmartFilterBar id="SmartFilterContracts" liveMode="false" entitySet="ZADOC_GRAINS_CONTRACT_DETAILS" persistencyKey="ContractsSmartFilterBar">                                                                          
                            <smartFilterBar:controlConfiguration>                                                                                                                                                                                              
                                <smartFilterBar:ControlConfiguration key="CompanyCode" label="Empresa" visibleInAdvancedArea="true"/>                                                                                                                          
                                <smartFilterBar:ControlConfiguration key="ContractNum" label="Número Contrato" visibleInAdvancedArea="true"/>                                                                                                                  
                                <smartFilterBar:ControlConfiguration key="ContractStatus" label="Status Contrato" visibleInAdvancedArea="true"/>                                                                                                               
                                <smartFilterBar:ControlConfiguration key="TradingContractType" label="Tipo Contrato" visibleInAdvancedArea="true"/>                                                                                                            
                                <smartFilterBar:ControlConfiguration key="CustomPartner" id="teste" label="Parceiro" visibleInAdvancedArea="true" groupId="basic" >                                                                                            
                                    <smartFilterBar:customControl>                                                                                                                                                                                             
                                        <Input id="Partner" valueHelpRequest="onValueHelpPartner" valueLiveUpdate="true" showValueHelp="true"/>                                                                                                                
                                    </smartFilterBar:customControl>                                                                                                                                                                                            
                                </smartFilterBar:ControlConfiguration>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbs+
p;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                                                     
                            </smartFilterBar:controlConfiguration>                                                                                                                                                                                             
                        </smartFilterBar:SmartFilterBar>                                                                                                                                                                                                       
                    </VBox>                                                                                                                                                                                                                                    
                </f:content>                                                                                                                                                                                                                                   
            </f:DynamicPageHeader>                                                                                                                                                                                                                             
        </f:header>                                                                                                                                                                                                                                            
        <f:content>                                                                                                                                                                                                                                            
            <smartTable:SmartTable                                                                                                                                                                                                                             
                id="SmartTableContracts"                                                                                                                                                                                                                       
                persistencyKey="SmartTableContracts"                                                                                                                                                                                                           
                entitySet="ZADOC_GRAINS_CONTRACT_DETAILS"                                                                                                                                                                                                      
                smartFilterId="SmartFilterContracts"                                                                                                                                                                                                           
                beforeRebindTable="onRebind"                                                                                                                                                                                                                   
                tableType="ResponsiveTable"                                                                                                                                                                                                                    
                header="Contratos"                                                                                                                                                                                                                             
                initiallyVisibleFields="CompanyCode,ContractNum,Material,SalesOrganization,TradingContractType,TradingContractTypeDescription,Quantity,ContractStatusText,Partner,PartnerName"                                                                 
                enableAutoBinding="false"                                                                                                                                                                                                                      
                requestAtLeastFields="ContractNum,TradingContractType,TradingContractTypeDescription,ContractStatus,InitialQuantity,ConsumedFixQuantiy,ContractStatusText,                                                                                     
                TradingContractCreatedBy,ContractCreatedByName,TradingContractCreatedOn,TradingContractChangedBy,ContractChangedByName,                                                                                                                        
                TradingContractChangedOn,CompanyCode,CompanyLocation,CompanyCountry,Amount,Currency,Safra,PaymentDate,Material,WareHouse,                                                                                                                      
                Quantity,Measure,Partner,PartnerName,PartnerCity,PartnerPostalCode,PartnerStreetNumber,IdentificationFix,SalesOrganization,Plant,AvailablePercentValue,PartnerBlockQuantity"                                                                   
            >                                                                                                                                                                                                                                                  
                <smartTable:customToolbar>                                                                                                                                                                                                                     
                    <OverflowToolbar design="Transparent">                                                                                                                                                                                                     
                        <ToolbarSpacer />                                                                                                                                                                                                                      
                    </OverflowToolbar>                                                                                                                                                                                                                         
                </smartTable:customToolbar>                                                                                                                                                                                                                    
                <Table id="TableContracts" mode="SingleSelectMaster" includeItemInSelection="true" growing="true" growingScrollToLoad="true" swipeDirection="LeftToRight" itemPress="onContractPress" selectionChange="onContractPress">                       
                    <columns>                                                                                                                                                                                                                                  
                        <Column id="PerQuantity">                                                                                                                                                                                                              
                            <customData>                                                                                                                                                                                                                       
                                <c:CustomData key="p13nData" value='\{"columnKey": "ConsumedPerQuantity", "autoColumnWidth": \{ "visibleProperty": "ConsumedPerQuantity" }, "columnIndex": 1 }' />                                                             
                            </customData>                                                                                                                                                                                                                      
                            <Text text="Consumido" />                                                                                                                                                                                                          
                        </Column>                                                                                                                                                                                                                              
                    </columns>                                                                                                                                                                                                                                 
                    <items>                                                                                                                                                                                                                                    
                        <ColumnListItem>                                                                                                                                                                                                                       
                            <cells>                                                                                                                                                                                                                            
                                <ProgressIndicator displayValue="{AvailablePercentValue}%" percentValue="{AvailablePercentValue}" state="{=${AvailablePercentValue} > 99 ? 'Success' : 'Warning'}" />&nbsp;                                                    
                            </cells>                                                                                                                                                                                                                           
                        </ColumnListItem>                                                                                                                                                                                                                      
                    </items>                                                                                                                                                                                                                                   
                </Table>                                                                                                                                                                                                                                       
            </smartTable:SmartTable>                                                                                                                                                                                                                           
        </f:content>                                                                                                                                                                                                                                           
    </f:DynamicPage>                                                                                                                                                                                                                                           
</c:FragmentDefinition>                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               