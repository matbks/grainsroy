<core:FragmentDefinition                                                                                                                                                                                                                                       
    xmlns="sap.m"                                                                                                                                                                                                                                              
    xmlns:core="sap.ui.core">                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
    <ViewSettingsDialog                                                                                                                                                                                                                                        
        id="viewSettingsDialog"                                                                                                                                                                                                                                
        confirm=".onConfirmViewSettingsDialog"                                                                                                                                                                                                                 
        sortDescending="true">                                                                                                                                                                                                                                 
        <sortItems>                                                                                                                                                                                                                                            
            <ViewSettingsItem                                                                                                                                                                                                                                  
                text="{/#ZADOC_GRAINS_CTR_APP_INV_SDType/TradingContract/@sap:label}"                                                                                                                                                                          
                key="TradingContract"                                                                                                                                                                                                                          
                />                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
            <ViewSettingsItem                                                                                                                                                                                                                                  
                text="{/#ZADOC_GRAINS_CTR_APP_INV_SDType/InvoiceCreatedOn/@sap:label}"                                                                                                                                                                         
                key="InvoiceCreatedOn"                                                                                                                                                                                                                         
                selected="true"/>                                                                                                                                                                                                                              
        </sortItems>                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
        <filterItems>                                                                                                                                                                                                                                          
            <ViewSettingsFilterItem                                                                                                                                                                                                                            
                id="filterItems"                                                                                                                                                                                                                               
                text="{/#ZADOC_GRAINS_CTR_APP_INV_SDType/AccountingDocumentStatus/@sap:label}"                                                                                                                                                                 
                multiSelect="false">                                                                                                                                                                                                                           
                <items>                                                                                                                                                                                                                                        
                    <ViewSettingsItem                                                                                                                                                                                                                          
                        id="OPEN"                                                                                                                                                                                                                              
                        text="{i18n>OPEN}"                                                                                                                                                                                                                     
                        key="OPEN"                                                                                                                                                                                                                             
                        selected="{= ${listView>/selectedStatus} === 'OPEN' ? true : false }"/>                                                                                                                                                                
                    <ViewSettingsItem                                                                                                                                                                                                                          
                        id="CLEARING"                                                                                                                                                                                                                          
                        text="{i18n>CLEARING}"                                                                                                                                                                                                                 
                        key="CLEARING"                                                                                                                                                                                                                         
                        selected="{= ${listView>/selectedStatus} === 'CLEARING' ? true : false }"/>                                                                                                                                                            
                </items>                                                                                                                                                                                                                                       
            </ViewSettingsFilterItem>                                                                                                                                                                                                                          
        </filterItems>                                                                                                                                                                                                                                         
        <groupItems>                                                                                                                                                                                                                                           
            <ViewSettingsItem                                                                                                                                                                                                                                  
                id="groupItems1"                                                                                                                                                                                                                               
                text="{/#ZADOC_GRAINS_CTR_APP_INV_SDType/TradingContract/@sap:label}"                                                                                                                                                                          
                key="TradingContract"/>                                                                                                                                                                                                                        
                 <ViewSettingsItem                                                                                                                                                                                                                             
                id="groupItems2"                                                                                                                                                                                                                               
                text="{/#ZADOC_GRAINS_CTR_APP_INV_SDType/InvoicePartner/@sap:label}"                                                                                                                                                                           
                key="InvoicePartner"/>                                                                                                                                                                                                                         
        </groupItems>                                                                                                                                                                                                                                          
    </ViewSettingsDialog>                                                                                                                                                                                                                                      
</core:FragmentDefinition>                                                                                                                                                                                                                                     