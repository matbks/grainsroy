<c:FragmentDefinition                                                                                                                                                                                                                                          
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:c="sap.ui.core"                                                                                                                                                                                                                                         
>                                                                                                                                                                                                                                                              
	<Dialog id="FixationDialog" contentWidth="900px" title="Dados da Fixação">&nbsp;                                                                                                                                                                              
		<beginButton>                                                                                                                                                                                                                                                
            <OverflowToolbarButton class="Button" icon="sap-icon://accept" type="Accept" press="onSaveFixation" tooltip="Salvar" />                                                                                                                            
        </beginButton>                                                                                                                                                                                                                                         
        <endButton>                                                                                                                                                                                                                                            
            <OverflowToolbarButton class="Button" icon="sap-icon://decline" type="Reject" press="onCloseFixation" tooltip="Cancelar" />                                                                                                                        
        </endButton>                                                                                                                                                                                                                                           
        <VBox class="Container">                                                                                                                                                                                                                               
            <Table id="FixationPopupTable" keyboardMode="Edit" includeItemInSelection="true">                                                                                                                                                                  
                <columns>                                                                                                                                                                                                                                      
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="Quantidade" />                                                                                                                                                                                                             
                    </Column>                                                                                                                                                                                                                                  
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="Sacas" />                                                                                                                                                                                                                  
                    </Column>                                                                                                                                                                                                                                  
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="Quantidade Disponível" />                                                                                                                                                                                                  
                    </Column>                                                                                                                                                                                                                                  
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="Preço Saca" />                                                                                                                                                                                                             
                    </Column>                                                                                                                                                                                                                                  
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="Preço Basis" />                                                                                                                                                                                                            
                    </Column>                                                                                                                                                                                                                                  
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="Preço Futuro" />                                                                                                                                                                                                           
                    </Column>                                                                                                                                                                                                                                  
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="Dólar" />                                                                                                                                                                                                                  
                    </Column>                                                                                                                                                                                                                                  
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="Valor Total" />                                                                                                                                                                                                            
                    </Column>                                                                                                                                                                                                                                  
                </columns>                                                                                                                                                                                                                                     
                <items>                                                                                                                                                                                                                                        
                    <ColumnListItem>                                                                                                                                                                                                                           
                        <cells>                                                                                                                                                                                                                                
                            <Input type="Number" value="{Fixation>/Quantity}" placeholder="KG" change="onChangeQuantity"/>                                                                                                                                     
                                                                                                                                                                                                                                                               
                            <ObjectNumber number="{path: 'Fixation>/Quantity', formatter: '.formatter.BagQuantity'}" unit="SC" />                                                                                                                              
                                                                                                                                                                                                                                                               
                            <ObjectNumber number="{path: 'Fixation>/AvailableQuantity', formatter: '.formatter.formatQuantity'}" unit="KG" />                                                                                                                  
                                                                                                                                                                                                                                                               
                            <Input value="{                                                                                                                                                                                                                    
                                path: 'Fixation>/BagPrice',                                                                                                                                                                                                    
                                type: 'sap.ui.model.type.Currency'}"&nbsp;                                                                                                                                                                                     
							/>                                                                                                                                                                                                                                                      
                                                                                                                                                                                                                                                               
                            <ObjectNumber                                                                                                                                                                                                                      
                                number="{parts: [{path: 'Fixation>/FuturePrice'},{path: 'Fixation>/BagPrice'},{path: 'ConversionsModel>/DolarQuotation'},{path: 'ConversionsModel>/CommodityFactor'}], formatter: '.formatter.formatBasisPrice'}"              
                                unit="{path: 'Fixation>/ConversionsCurrency'}"                                                                                                                                                                                 
                            />                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
                            <ObjectNumber number="{path: 'Fixation>/FuturePrice', formatter: '.formatter.formatCurrencyDecimal'}" unit="{path: 'Fixation>/ConversionsCurrency'}" />                                                                            
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                       
                            <ObjectNumber number="{path:'ConversionsModel>/DolarQuotation', formatter: '.formatter.formatCurrencyDecimal'}" unit="{Fixation>/ConversionsCurrency}"/>                                                                           
                                                                                                                                                                                                                                                               
                            <ObjectNumber number="{parts: [{path: 'Fixation>/Quantity'},{path: 'Fixation>/BagPrice'},{path: 'Fixation>/BlockedQuantity'}, {path: 'ConversionsModel>/RoyaltiesBlockPerc'}], formatter: '.formatter.formatFinalAmount'}"/>       
                        </cells>                                                                                                                                                                                                                               
                    </ColumnListItem>                                                                                                                                                                                                                          
                </items>                                                                                                                                                                                                                                       
            </Table>                                                                                                                                                                                                                                           
        </VBox>                                                                                                                                                                                                                                                
        <MessageStrip id="MessageStrip" visible="false" class="sapUiSmallMarginBeginEnd sapUiSmallMarginTopBottom" text="Information with custom icon" type="Error" customIcon="sap-icon://warning" showIcon="true" />	                                        
	</Dialog>                                                                                                                                                                                                                                                     
</c:FragmentDefinition>                                                                                                                                                                                                                                        