sap.ui.define([], function () {                                                                                                                                                                                                                                
    "use strict";                                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
    return {                                                                                                                                                                                                                                                   
        BagQuantity: function (Quantity) {                                                                                                                                                                                                                     
            // return Quantity / 60 - Math.floor(Quantity / 60) > 0.5 ? Math.ceil(Quantity / 60) : Math.floor(Quantity / 60)                                                                                                                                   
            let bagQuantity = parseFloat(Quantity / 60).toFixed(4)                                                                                                                                                                                             
            let oQuantityFormat = sap.ui.core.format.NumberFormat.getFloatInstance({                                                                                                                                                                           
                "groupingEnabled": true,                                                                                                                                                                                                                       
                "groupingSeparator": '.',                                                                                                                                                                                                                      
                "groupingSize": 3,                                                                                                                                                                                                                             
                "decimalSeparator": ","                                                                                                                                                                                                                        
            })                                                                                                                                                                                                                                                 
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                       
            return oQuantityFormat.format(bagQuantity)                                                                                                                                                                                                         
        },                                                                                                                                                                                                                                                     
        formatAmount: function (Quantity, BagPrice) {                                                                                                                                                                                                          
            let oCurrencyFormat = sap.ui.core.format.NumberFormat.getCurrencyInstance({                                                                                                                                                                        
                currencyCode: "BRL"                                                                                                                                                                                                                            
            });                                                                                                                                                                                                                                                
            BagPrice = BagPrice.toString()                                                                                                                                                                                                                     
            BagPrice = parseFloat(BagPrice.replace(',', '.'))                                                                                                                                                                                                  
            return oCurrencyFormat.format(Quantity * BagPrice / 60);                                                                                                                                                                                           
        },                                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
        formatBlockedAmount: function (BlockedQuantity, BagPrice, RoyaltiesPerc) {                                                                                                                                                                             
            let oCurrencyFormat = sap.ui.core.format.NumberFormat.getCurrencyInstance({                                                                                                                                                                        
                currencyCode: "BRL"                                                                                                                                                                                                                            
            });                                                                                                                                                                                                                                                
                                                                                                                                                                                                                                                               
            return oCurrencyFormat.format(BlockedQuantity * (BagPrice[0] / 60) * RoyaltiesPerc)                                                                                                                                                                
        },                                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
        formatFinalAmount: function (Quantity, BagPrice, BlockedQuantity, RoyaltiesPerc) {                                                                                                                                                                     
            let oCurrencyFormat = sap.ui.core.format.NumberFormat.getCurrencyInstance({                                                                                                                                                                        
                currencyCode: "BRL"                                                                                                                                                                                                                            
            });                                                                                                                                                                                                                                                
            return oCurrencyFormat.format((Quantity * (BagPrice[0] / 60)) - (BlockedQuantity * (BagPrice[0] / 60) * RoyaltiesPerc))                                                                                                                            
        },                                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
        formatQuantity: function (Quantity) {                                                                                                                                                                                                                  
            let oQuantityFormat = sap.ui.core.format.NumberFormat.getFloatInstance({                                                                                                                                                                           
                "groupingEnabled": true,                                                                                                                                                                                                                       
                "groupingSeparator": '.',                                                                                                                                                                                                                      
                "groupingSize": 3,                                                                                                                                                                                                                             
                "decimalSeparator": ","&nbsp;                                                                                                                                                                                                                  
            })                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
            return oQuantityFormat.format(Quantity)                                                                                                                                                                                                            
        },                                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
        formatBasisPrice: function (FuturePrice, BagPrice, Dolar, Factor) {                                                                                                                                                                                    
            BagPrice = BagPrice.toString()                                                                                                                                                                                                                     
            BagPrice = parseFloat(BagPrice.replace(',', '.'))                                                                                                                                                                                                  
            let BasisPrice = (((BagPrice / Dolar) / Factor) - FuturePrice)                                                                                                                                                                                     
            let Price = parseFloat(BagPrice !== 0 ? BasisPrice : 0)                                                                                                                                                                                            
            return Price.toFixed(4)                                                                                                                                                                                                                            
        },                                                                                                                                                                                                                                                     
        formatCurrency: function (Amount) {                                                                                                                                                                                                                    
            let oCurrencyFormat = sap.ui.core.format.NumberFormat.getCurrencyInstance({                                                                                                                                                                        
                currencyCode: "BRL"                                                                                                                                                                                                                            
            });                                                                                                                                                                                                                                                
            return oCurrencyFormat.format(Amount);                                                                                                                                                                                                             
        },                                                                                                                                                                                                                                                     
        formatCurrencyDecimal: function (Amount) {                                                                                                                                                                                                             
            Amount = parseFloat(Amount)                                                                                                                                                                                                                        
            return Amount.toFixed(4)                                                                                                                                                                                                                           
        },                                                                                                                                                                                                                                                     
        formatFixationStatus: function (Status) {                                                                                                                                                                                                              
            switch (Status) {                                                                                                                                                                                                                                  
                case 'U':                                                                                                                                                                                                                                      
                    return "sap-icon://customer-history"                                                                                                                                                                                                       
                case 'S':                                                                                                                                                                                                                                      
                    return "sap-icon://thumb-down"                                                                                                                                                                                                             
                case 'T':                                                                                                                                                                                                                                      
                    return "sap-icon://thumb-up"                                                                                                                                                                                                               
                case 'L':                                                                                                                                                                                                                                      
                    return "sap-icon://loan"                                                                                                                                                                                                                   
                case 'F':                                                                                                                                                                                                                                      
                    return "sap-icon://my-sales-order"                                                                                                                                                                                                         
                case 'C':                                                                                                                                                                                                                                      
                    return "sap-icon://collections-insight"                                                                                                                                                                                                    
                default:                                                                                                                                                                                                                                       
                    return ''                                                                                                                                                                                                                                  
            }                                                                                                                                                                                                                                                  
        },                                                                                                                                                                                                                                                     
        formatFixStatusState: function (Status) {                                                                                                                                                                                                              
            switch (Status) {                                                                                                                                                                                                                                  
                case 'U':                                                                                                                                                                                                                                      
                    return "Warning"                                                                                                                                                                                                                           
                case 'S':                                                                                                                                                                                                                                      
                    return "Error"                                                                                                                                                                                                                             
                case 'T':                                                                                                                                                                                                                                      
                    return "Success"                                                                                                                                                                                                                           
                case 'L':                                                                                                                                                                                                                                      
                    return "Success"                                                                                                                                                                                                                           
                case 'F':                                                                                                                                                                                                                                      
                    return "Success"                                                                                                                                                                                                                           
                case 'C':                                                                                                                                                                                                                                      
                    return "Success"                                                                                                                                                                                                                           
                default:                                                                                                                                                                                                                                       
                    return 'None'                                                                                                                                                                                                                              
            }                                                                                                                                                                                                                                                  
        },                                                                                                                                                                                                                                                     
        formatLiqVisible: function (Status) {                                                                                                                                                                                                                  
            return Status == 'T' ? true : false                                                                                                                                                                                                                
        },&nbsp;                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
        typeButtonGoToApprove: function (Status) {                                                                                                                                                                                                             
            var type = sap.m.ButtonType.Success;                                                                                                                                                                                                               
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                       
            if (Status == 'U') {                                                                                                                                                                                                                               
                type = sap.m.ButtonType.Default                                                                                                                                                                                                                
            } else if (Status == 'S'){                                                                                                                                                                                                                         
                type = sap.m.ButtonType.Reject                                                                                                                                                                                                                 
            };                                                                                                                                                                                                                                                 
            return type                                                                                                                                                                                                                                        
        },&nbsp;                                                                                                                                                                                                                                               
        typeButtonLiquidate: function (Status) {                                                                                                                                                                                                               
            var type = sap.m.ButtonType.Success;                                                                                                                                                                                                               
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                       
            if (Status == 'U' || Status == 'T' || Status == 'S') {                                                                                                                                                                                             
                type = sap.m.ButtonType.Default                                                                                                                                                                                                                
            };                                                                                                                                                                                                                                                 
            return type                                                                                                                                                                                                                                        
        },&nbsp;                                                                                                                                                                                                                                               
        typeButtonInvoicePost: function (Status) {                                                                                                                                                                                                             
            var type = sap.m.ButtonType.Default;                                                                                                                                                                                                               
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                       
            if (Status == 'C' || Status == 'F') {                                                                                                                                                                                                              
                type = sap.m.ButtonType.Success                                                                                                                                                                                                                
            };                                                                                                                                                                                                                                                 
            return type                                                                                                                                                                                                                                        
        },&nbsp;                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
        typeButtonClearing: function (Status) {                                                                                                                                                                                                                
            var type = sap.m.ButtonType.Default;                                                                                                                                                                                                               
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                       
            if (Status == 'C') {                                                                                                                                                                                                                               
                type = sap.m.ButtonType.Success                                                                                                                                                                                                                
            };                                                                                                                                                                                                                                                 
            return type                                                                                                                                                                                                                                        
        },                                                                                                                                                                                                                                                     
        formatProperty: function(Property){                                                                                                                                                                                                                    
            if(Property != ""){                                                                                                                                                                                                                                
                return 'Propriedade: ' + Property                                                                                                                                                                                                              
            }                                                                                                                                                                                                                                                  
            return ''                                                                                                                                                                                                                                          
        }&nbsp;                                                                                                                                                                                                                                                
    };                                                                                                                                                                                                                                                         
});                                                                                                                                                                                                                                                            