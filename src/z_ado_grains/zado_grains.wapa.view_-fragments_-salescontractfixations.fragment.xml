<c:FragmentDefinition xmlns="sap.m" xmlns:uxap="sap.uxap" xmlns:c="sap.ui.core" xmlns:smartTable="sap.ui.comp.smarttable">                                                                                                                                     
    <uxap:ObjectPageLayout showTitleInHeaderContent="true" showEditHeaderButton="true" upperCaseAnchorBar="false" preserveHeaderStateOnScroll="true" id="SalesContractFixationDetailPage">                                                                     
        <uxap:headerTitle>                                                                                                                                                                                                                                     
            <uxap:ObjectPageDynamicHeaderTitle>                                                                                                                                                                                                                
                <uxap:expandedHeading>                                                                                                                                                                                                                         
                    <HBox>                                                                                                                                                                                                                                     
                        <Avatar src="sap-icon://sales-document" class="sapUiMediumMarginEnd" />                                                                                                                                                                
                        <Title text="Detalhes Fixação {FixationId} - Contrato {ContractTypeDescription} Nº {ContractNum}" class="sapUiTinyMarginTop" wrapping="true" />                                                                                        
                    </HBox>                                                                                                                                                                                                                                    
                </uxap:expandedHeading>                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               
                <uxap:snappedHeading>                                                                                                                                                                                                                          
                    <FlexBox fitContainer="true">                                                                                                                                                                                                              
                        <Title text="Detalhes Fixação {FixationId} - Contrato {ContractTypeDescription} Nº {ContractNum}" wrapping="true" />                                                                                                                   
                    </FlexBox>                                                                                                                                                                                                                                 
                </uxap:snappedHeading>                                                                                                                                                                                                                         
                <uxap:actions>                                                                                                                                                                                                                                 
                    <Button text="Liquidar" type="Success" press="onLiquidation" visible="{path: 'FixationStatus', formatter: '.formatter.formatLiqVisible'}"&nbsp;                                                                                            
                        icon="sap-icon://monitor-payments"&nbsp;                                                                                                                                                                                               
                    />                                                                                                                                                                                                                                         
                    <Button text="Boletim fixação" type="Emphasized" press="onPrintForm" icon="sap-icon://print" visible="{=${FixationStatus == 'L' ? true : false}}"/>                                                                                        
                </uxap:actions>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                        
            </uxap:ObjectPageDynamicHeaderTitle>                                                                                                                                                                                                               
        </uxap:headerTitle>                                                                                                                                                                                                                                    
        <uxap:headerContent>                                                                                                                                                                                                                                   
            <HeaderContainer scrollStep="200" showDividers="false" showOverflowItem="false">                                                                                                                                                                   
                <VBox class="sapUiLargeMarginEnd sapUiSmallMarginBottom">                                                                                                                                                                                      
                    <Title text="Informações Gerais" class="title sapUiTinyMarginBottom" />                                                                                                                                                                    
                                                                                                                                                                                                                                                               
                    <HBox class="sapUiTinyMarginBottom">                                                                                                                                                                                                       
                        <c:Icon src="sap-icon://building" />                                                                                                                                                                                                   
                        <Title text="{CompanyCode} - {CompanyCodeName}" class="sapUiSmallMarginBegin" />                                                                                                                                                       
                    </HBox>                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               
                    <HBox>                                                                                                                                                                                                                                     
                        <c:Icon src="sap-icon://map" />                                                                                                                                                                                                        
                        <Title text="{CompanyLocation}" class="sapUiSmallMarginBegin" />                                                                                                                                                                       
                    </HBox>                                                                                                                                                                                                                                    
                </VBox>                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               
                <VBox class="sapUiSmallMarginBeginEnd sapUiSmallMarginBottom">                                                                                                                                                                                 
                    <Title text="Status" class="sapUiTinyMarginBottom" />                                                                                                                                                                                      
                    <ObjectStatus text="{FixationStatusDescription}" state="{path: 'FixationStatus', formatter: '.formatter.formatFixStatusState'}" icon="{path:'FixationStatus', formatter: '.formatter.formatFixationStatus'}" class="sapMObjectStatusLarge"+
 />                                                                                                                                                                                                                                                            
                </VBox>                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               
                <VBox class="sapUiSmallMarginBeginEnd sapUiSmallMarginBottom">                                                                                                                                                                                 
                    <Title text="Parceiro" class="sapUiTinyMarginBottom" />                                                                                                                                                                                    
                    <ObjectStatus text="{PartnerName}" state="Success" class="sapMObjectStatusLarge" />                                                                                                                                                        
                </VBox>                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               
                <VBox class="sapUiSmallMarginBeginEnd sapUiSmallMarginBottom">                                                                                                                                                                                 
                    <Title text="Quantidade fixação" class="sapUiTinyMarginBottom" />                                                                                                                                                                          
                    <ObjectStatus text="{path:'FixationQuantity', formatter: '.formatter.formatQuantity'} KG" state="Success" class="sapMObjectStatusLarge" />                                                                                                 
                </VBox>                                                                                                                                                                                                                                        
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                               
                <VBox class="sapUiSmallMarginBeginEnd sapUiSmallMarginBottom">                                                                                                                                                                                 
                    <Title text="Montante" class="sapUiTinyMarginBottom" />                                                                                                                                                                                    
                    <ObjectStatus text="{path:'FixationAmount', formatter: '.formatter.formatCurrency'} BRL" state="Success" class="sapMObjectStatusLarge" />                                                                                                  
                </VBox>                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               
                <VBox class="sapUiSmallMarginBeginEnd sapUiSmallMarginBottom">                                                                                                                                                                                 
                    <Title text="Material" class="sapUiTinyMarginBottom" />                                                                                                                                                                                    
                    <ObjectStatus text="{Material}" icon="sap-icon://e-care" class="sapMObjectStatusLarge" />                                                                                                                                                  
                </VBox>                                                                                                                                                                                                                                        
            </HeaderContainer>                                                                                                                                                                                                                                 
        </uxap:headerContent>                                                                                                                                                                                                                                  
        <uxap:sections>                                                                                                                                                                                                                                        
            <uxap:ObjectPageSection titleUppercase="false" title="Dados Gerais">                                                                                                                                                                               
                <uxap:subSections>                                                                                                                                                                                                                             
                    <uxap:ObjectPageSubSection>                                                                                                                                                                                                                
                        <VBox class="sapUiTyniMarginBeginEnd sapUiTinyMarginTop ">                                                                                                                                                                             
                            <Title text="Detalhes da Aplicação" class="title sapUiMediumMarginBottom" />                                                                                                                                                       
                            <HBox class="sapUiTinyMarginBottom">                                                                                                                                                                                               
                                <Label text="Criado por:" class="sapUiSmallMarginEnd" />                                                                                                                                                                       
                                <Text text="{CreatedBy}" />                                                                                                                                                                                                    
                            </HBox>                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
                            <HBox class="sapUiTinyMarginBottom">                                                                                                                                                                                               
                                <Label text="Criado em:" class="sapUiSmallMarginEnd" />                                                                                                                                                                        
                                <Text text="{CreatedOn}" />                                                                                                                                                                                                    
                            </HBox>                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
                            <HBox class="sapUiTinyMarginBottom">                                                                                                                                                                                               
                                <Label text="Modificado por:" class="sapUiSmallMarginEnd" />                                                                                                                                                                   
                                <Text text="{ChangedBy}" />                                                                                                                                                                                                    
                            </HBox>                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
                            <HBox class="sapUiTinyMarginBottom">                                                                                                                                                                                               
                                <Label text="Modificado em:" class="sapUiSmallMarginEnd" />                                                                                                                                                                    
                                <Text text="{ChangedOn}" />                                                                                                                                                                                                    
                            </HBox>                                                                                                                                                                                                                            
                        </VBox>                                                                                                                                                                                                                                
                    </uxap:ObjectPageSubSection>                                                                                                                                                                                                               
                </uxap:subSections>                                                                                                                                                                                                                            
            </uxap:ObjectPageSection>                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
            <uxap:ObjectPageSection titleUppercase="false" title="Itens da fixação">                                                                                                                                                                           
                <uxap:subSections>                                                                                                                                                                                                                             
                    <uxap:ObjectPageSubSection>                                                                                                                                                                                                                
                        <VBox class="Container">                                                                                                                                                                                                               
                            <smartTable:SmartTable                                                                                                                                                                                                             
                            id="SmartTableFixationData"                                                                                                                                                                                                        
                                persistencyKey="SmartTableContracts"                                                                                                                                                                                           
                                entitySet="ZADOC_GRAINS_FIXATION_DATA"                                                                                                                                                                                         
                                tableType="ResponsiveTable"                                                                                                                                                                                                    
                                initiallyVisibleFields="ContractNum,IdentificationFix,Plant,Material,Fixation,                                                                                                                                                 
                                Transgenic,Quantity,Unit,Amount,Bagprice,BasisPrice,FuturePrice,Currency,Property,Status"                                                                                                                                      
                                enableAutoBinding="true"                                                                                                                                                                                                       
                                requestAtLeastFields="ContractNum,IdentificationFix"                                                                                                                                                                           
                                tableBindingPath="to_FixationData"                                                                                                                                                                                             
                            >                                                                                                                                                                                                                                  
                            </smartTable:SmartTable>                                                                                                                                                                                                           
                        </VBox>                                                                                                                                                                                                                                
                    </uxap:ObjectPageSubSection>                                                                                                                                                                                                               
                </uxap:subSections>                                                                                                                                                                                                                            
            </uxap:ObjectPageSection>                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
            <uxap:ObjectPageSection titleUppercase="false" title="Dados de Aplicações">                                                                                                                                                                        
                <uxap:subSections>                                                                                                                                                                                                                             
                    <uxap:ObjectPageSubSection>                                                                                                                                                                                                                
                        <smartTable:SmartTable                                                                                                                                                                                                                 
                            id="SmartTableApplications"                                                                                                                                                                                                        
                            persistencyKey="SmartTableContracts"                                                                                                                                                                                               
                            entitySet="ZADOC_GRAINS_CONSUMED_APPL"                                                                                                                                                                                             
                            tableType="ResponsiveTable"                                                                                                                                                                                                        
                            initiallyVisibleFields="ContractNum,FixationId,ApplicationDoc,EdcNum,Plant,Property,Quantity,Unit"                                                                                                                                 
                            enableAutoBinding="true"                                                                                                                                                                                                           
                            requestAtLeastFields="ContractNum,FixationId"                                                                                                                                                                                      
                            tableBindingPath="to_ConsApplications"                                                                                                                                                                                             
                        />                                                                                                                                                                                                                                     
                    </uxap:ObjectPageSubSection>                                                                                                                                                                                                               
                </uxap:subSections>                                                                                                                                                                                                                            
            </uxap:ObjectPageSection>                                                                                                                                                                                                                          
        </uxap:sections>                                                                                                                                                                                                                                       
    </uxap:ObjectPageLayout>                                                                                                                                                                                                                                   
</c:FragmentDefinition>                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               