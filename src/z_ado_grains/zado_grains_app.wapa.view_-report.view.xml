<mvc:View                                                                                                                                                                                                                                                      
	controllerName="grains.zadograinsacmcontrollingapproval.controller.Report"                                                                                                                                                                                    
	xmlns:mvc="sap.ui.core.mvc"                                                                                                                                                                                                                                   
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:core="sap.ui.core"                                                                                                                                                                                                                                      
	xmlns:f="sap.f"                                                                                                                                                                                                                                               
	displayBlock="true"                                                                                                                                                                                                                                           
	height="100%"                                                                                                                                                                                                                                                 
	busyIndicatorDelay="0"                                                                                                                                                                                                                                        
	xmlns:smartFilterBar="sap.ui.comp.smartfilterbar"                                                                                                                                                                                                             
	xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"                                                                                                                                                                           
>                                                                                                                                                                                                                                                              
	<f:DynamicPage                                                                                                                                                                                                                                                
		id="dynamicPageId"                                                                                                                                                                                                                                           
		preserveHeaderStateOnScroll="true"                                                                                                                                                                                                                           
		headerExpanded="{/headerExpanded}"                                                                                                                                                                                                                           
		class="sapUiNoContentPadding"                                                                                                                                                                                                                                
		toggleHeaderOnTitleClick="{/titleClickable}"                                                                                                                                                                                                                 
	>                                                                                                                                                                                                                                                             
		<!-- DynamicPage Title -->                                                                                                                                                                                                                                   
		<f:title>                                                                                                                                                                                                                                                    
			<f:DynamicPageTitle id="dynamicPageTitle">                                                                                                                                                                                                                  
				<f:heading>                                                                                                                                                                                                                                                
					<Title                                                                                                                                                                                                                                                    
						id="Title"                                                                                                                                                                                                                                               
						text="{i18n>report}"                                                                                                                                                                                                                                     
					/>                                                                                                                                                                                                                                                        
				</f:heading>                                                                                                                                                                                                                                               
				<f:expandedContent/>                                                                                                                                                                                                                                       
				<f:snappedContent/>                                                                                                                                                                                                                                        
				<f:snappedTitleOnMobile/>                                                                                                                                                                                                                                  
				<f:content/>                                                                                                                                                                                                                                               
				<f:actions/>                                                                                                                                                                                                                                               
			</f:DynamicPageTitle>                                                                                                                                                                                                                                       
		</f:title>                                                                                                                                                                                                                                                   
		<!-- DynamicPage Header -->                                                                                                                                                                                                                                  
		<f:header>                                                                                                                                                                                                                                                   
			<f:DynamicPageHeader                                                                                                                                                                                                                                        
				id="dynamicPageHeader"                                                                                                                                                                                                                                     
				pinnable="true"                                                                                                                                                                                                                                            
			>                                                                                                                                                                                                                                                           
				<smartFilterBar:SmartFilterBar                                                                                                                                                                                                                             
					id="smartFilterBarReport"                                                                                                                                                                                                                                 
					entitySet="ZADOC_GRAINS_CONTR_APPROV_HIST"                                                                                                                                                                                                                
					persistencyKey="SmartFilter_Explored"                                                                                                                                                                                                                     
					liveMode="true"                                                                                                                                                                                                                                           
				>                                                                                                                                                                                                                                                          
					<smartFilterBar:controlConfiguration>                                                                                                                                                                                                                     
						<smartFilterBar:ControlConfiguration                                                                                                                                                                                                                     
							key="Type"                                                                                                                                                                                                                                              
							visibleInAdvancedArea="true"                                                                                                                                                                                                                            
						>                                                                                                                                                                                                                                                        
							<smartFilterBar:customControl>                                                                                                                                                                                                                          
								<MultiComboBox                                                                                                                                                                                                                                         
									id="multiComboBox"                                                                                                                                                                                                                                    
									customData:hasValue="true"                                                                                                                                                                                                                            
								>                                                                                                                                                                                                                                                      
									<core:Item                                                                                                                                                                                                                                            
										id="contrato"                                                                                                                                                                                                                                        
										key="Contrato"                                                                                                                                                                                                                                       
										text="Contrato"                                                                                                                                                                                                                                      
									/>                                                                                                                                                                                                                                                    
									<core:Item                                                                                                                                                                                                                                            
										id="fixacao"                                                                                                                                                                                                                                         
										key="Fixação"                                                                                                                                                                                                                                        
										text="Fixação"                                                                                                                                                                                                                                       
									/>                                                                                                                                                                                                                                                    
								</MultiComboBox>                                                                                                                                                                                                                                       
							</smartFilterBar:customControl>                                                                                                                                                                                                                         
						</smartFilterBar:ControlConfiguration>                                                                                                                                                                                                                   
					</smartFilterBar:controlConfiguration>                                                                                                                                                                                                                    
					<smartFilterBar:controlConfiguration>                                                                                                                                                                                                                     
						<smartFilterBar:ControlConfiguration                                                                                                                                                                                                                     
							key="TradingContractNumber"                                                                                                                                                                                                                             
							visibleInAdvancedArea="true"                                                                                                                                                                                                                            
							id="filterContract"                                                                                                                                                                                                                                     
						/>                                                                                                                                                                                                                                                       
					</smartFilterBar:controlConfiguration>                                                                                                                                                                                                                    
					<smartFilterBar:controlConfiguration>                                                                                                                                                                                                                     
						<smartFilterBar:ControlConfiguration                                                                                                                                                                                                                     
							key="CreatedByUserName"                                                                                                                                                                                                                                 
							visibleInAdvancedArea="true"                                                                                                                                                                                                                            
							id="filterCreatedBy"                                                                                                                                                                                                                                    
						/>                                                                                                                                                                                                                                                       
					</smartFilterBar:controlConfiguration>                                                                                                                                                                                                                    
					<smartFilterBar:controlConfiguration>                                                                                                                                                                                                                     
						<smartFilterBar:ControlConfiguration                                                                                                                                                                                                                     
							key="PreviousStatus"                                                                                                                                                                                                                                    
							visibleInAdvancedArea="true"                                                                                                                                                                                                                            
							id="filterPreviousStatus"                                                                                                                                                                                                                               
						/>                                                                                                                                                                                                                                                       
					</smartFilterBar:controlConfiguration>                                                                                                                                                                                                                    
				</smartFilterBar:SmartFilterBar>                                                                                                                                                                                                                           
			</f:DynamicPageHeader>                                                                                                                                                                                                                                      
		</f:header>                                                                                                                                                                                                                                                  
		<f:content>                                                                                                                                                                                                                                                  
			<VBox                                                                                                                                                                                                                                                       
				class="sapUiNoContentPadding"                                                                                                                                                                                                                              
				fitContainer="true"                                                                                                                                                                                                                                        
				height="100%"                                                                                                                                                                                                                                              
				width="100%"                                                                                                                                                                                                                                               
			>                                                                                                                                                                                                                                                           
				<smartTable:SmartTable                                                                                                                                                                                                                                     
					xmlns:smartTable="sap.ui.comp.smarttable"                                                                                                                                                                                                                 
					xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"                                                                                                                                                                              
					id="smartTableReport"                                                                                                                                                                                                                                     
					entitySet="ZADOC_GRAINS_CONTR_APPROV_HIST"                                                                                                                                                                                                                
					smartFilterId="smartFilterBarReport"                                                                                                                                                                                                                      
					tableType="Table"                                                                                                                                                                                                                                         
					header="{i18n>items}"                                                                                                                                                                                                                                     
					persistencyKey="SmartTableAnalytical_Explored"                                                                                                                                                                                                            
					class="sapUiResponsiveContentPadding"                                                                                                                                                                                                                     
					app:useSmartToggle="true"                                                                                                                                                                                                                                 
					initiallyVisibleFields="Type,TradingContractNumber,StepDescription,PreviousStatus,NewStatus,CreatedByUser,CreatedOn"                                                                                                                                      
					enableAutoBinding="true"                                                                                                                                                                                                                                  
					showFullScreenButton="true"                                                                                                                                                                                                                               
					beforeRebindTable="onBeforeRebindSmartTableReport"                                                                                                                                                                                                        
					requestAtLeastFields="MessageNumber,Guid"                                                                                                                                                                                                                 
				>                                                                                                                                                                                                                                                          
					<smartTable:customToolbar>                                                                                                                                                                                                                                
						<OverflowToolbar                                                                                                                                                                                                                                         
							id="overflowToolbar"                                                                                                                                                                                                                                    
							design="Transparent"                                                                                                                                                                                                                                    
						>                                                                                                                                                                                                                                                        
							<ToolbarSpacer id="toolbarSpacer"/>                                                                                                                                                                                                                     
							<OverflowToolbarButton                                                                                                                                                                                                                                  
								id="removeSelections"                                                                                                                                                                                                                                  
								icon="sap-icon://multiselect-none"                                                                                                                                                                                                                     
								tooltip="{i18n>removeSelections}"                                                                                                                                                                                                                      
								press="onRemoveSelections"                                                                                                                                                                                                                             
								visible="false"                                                                                                                                                                                                                                        
							/>                                                                                                                                                                                                                                                      
							<OverflowToolbarButton                                                                                                                                                                                                                                  
								id="refresh"                                                                                                                                                                                                                                           
								icon="sap-icon://refresh"                                                                                                                                                                                                                              
								tooltip="{i18n>refresh}"                                                                                                                                                                                                                               
								press="onRefreshSmartTable"                                                                                                                                                                                                                            
							/>                                                                                                                                                                                                                                                      
						</OverflowToolbar>                                                                                                                                                                                                                                       
					</smartTable:customToolbar>                                                                                                                                                                                                                               
					<Table                                                                                                                                                                                                                                                    
						id="tableReport"                                                                                                                                                                                                                                         
						mode="SingleSelectMaster"                                                                                                                                                                                                                                
						includeItemInSelection="true"                                                                                                                                                                                                                            
						contextualWidth="auto"                                                                                                                                                                                                                                   
						autoPopinMode="true"                                                                                                                                                                                                                                     
						growing="true"                                                                                                                                                                                                                                           
						growingScrollToLoad="true"                                                                                                                                                                                                                               
						sticky="ColumnHeaders,HeaderToolbar"                                                                                                                                                                                                                     
						itemPress="onRowPress"                                                                                                                                                                                                                                   
					>                                                                                                                                                                                                                                                         
						<columns/>                                                                                                                                                                                                                                               
						<items>                                                                                                                                                                                                                                                  
							<ColumnListItem                                                                                                                                                                                                                                         
								type="Active"                                                                                                                                                                                                                                          
								id="columnListItem"                                                                                                                                                                                                                                    
								highlight="{                                                                                                                                                                                                                                           
									path: 'MessageNumber',                                                                                                                                                                                                                                
									formatter: '.formatRowHighlight'                                                                                                                                                                                                                      
								}"                                                                                                                                                                                                                                                     
							>                                                                                                                                                                                                                                                       
								<cells/>                                                                                                                                                                                                                                               
							</ColumnListItem>                                                                                                                                                                                                                                       
						</items>                                                                                                                                                                                                                                                 
					</Table>                                                                                                                                                                                                                                                  
				</smartTable:SmartTable>                                                                                                                                                                                                                                   
			</VBox>                                                                                                                                                                                                                                                     
		</f:content>                                                                                                                                                                                                                                                 
		<f:footer/>                                                                                                                                                                                                                                                  
	</f:DynamicPage>                                                                                                                                                                                                                                              
</mvc:View>                                                                                                                                                                                                                                                    