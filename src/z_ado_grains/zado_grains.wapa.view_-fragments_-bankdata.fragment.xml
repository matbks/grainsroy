<c:FragmentDefinition xmlns="sap.m" xmlns:c="sap.ui.core">                                                                                                                                                                                                     
    <Dialog title="Informações Bancárias" id="BankDataDialog" contentWidth="1300px" resizable="true">                                                                                                                                                          
        <beginButton>                                                                                                                                                                                                                                          
            <OverflowToolbarButton class="Button" icon="sap-icon://accept" type="Accept" press="onSaveBankData" tooltip="Salvar" />                                                                                                                            
        </beginButton>                                                                                                                                                                                                                                         
        <endButton>                                                                                                                                                                                                                                            
            <OverflowToolbarButton class="Button" icon="sap-icon://decline" type="Reject" press="onCloseBankData" tooltip="Cancelar" />                                                                                                                        
        </endButton>                                                                                                                                                                                                                                           
        <VBox class="Container">                                                                                                                                                                                                                               
            <Table id="BankDataPopupTable" keyboardMode="Edit" mode="SingleSelectMaster" includeItemInSelection="true" items="{BankData>/items/}">                                                                                                             
                <headerToolbar>                                                                                                                                                                                                                                
                    <OverflowToolbar design="Transparent">                                                                                                                                                                                                     
                        <OverflowToolbarButton class="Button" text="Nova Conta" icon="sap-icon://add" press="onAddLineFixation" />                                                                                                                             
                        <OverflowToolbarButton class="Button" icon="sap-icon://delete" press="onDeleteLineFixation"/>                                                                                                                                          
                        <content>                                                                                                                                                                                                                              
                            <ObjectStatus                                                                                                                                                                                                                      
                                class="sapUiMediumMarginBegin"                                                                                                                                                                                                 
                                state="Success"                                                                                                                                                                                                                
                                text="{path: 'Fixation>/Amount', formatter: '.formatter.formatCurrency'} BRL"                                                                                                                                                  
                                title="Valor Total"                                                                                                                                                                                                            
                            />                                                                                                                                                                                                                                 
                        </content>                                                                                                                                                                                                                             
                    </OverflowToolbar>                                                                                                                                                                                                                         
                </headerToolbar>                                                                                                                                                                                                                               
                <columns>                                                                                                                                                                                                                                      
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="Parcela"/>                                                                                                                                                                                                                 
                    </Column>                                                                                                                                                                                                                                  
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="Valor"/>                                                                                                                                                                                                                   
                    </Column>                                                                                                                                                                                                                                  
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="Data Pagamento"/>                                                                                                                                                                                                          
                    </Column>                                                                                                                                                                                                                                  
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="Selecionar"/>                                                                                                                                                                                                              
                    </Column>                                                                                                                                                                                                                                  
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="Banco"/>                                                                                                                                                                                                                   
                    </Column>                                                                                                                                                                                                                                  
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="Cidade"/>                                                                                                                                                                                                                  
                    </Column>                                                                                                                                                                                                                                  
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="Agência"/>                                                                                                                                                                                                                 
                    </Column>                                                                                                                                                                                                                                  
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="Conta"/>                                                                                                                                                                                                                   
                    </Column>                                                                                                                                                                                                                                  
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="Titular"/>                                                                                                                                                                                                                 
                    </Column>                                                                                                                                                                                                                                  
                </columns>                                                                                                                                                                                                                                     
                <items>                                                                                                                                                                                                                                        
                    <ColumnListItem>                                                                                                                                                                                                                           
                        <cells>                                                                                                                                                                                                                                
                            <Input value="{BankData>Parcel}" type="Number"/>                                                                                                                                                                                   
                        </cells>                                                                                                                                                                                                                               
                        <cells>                                                                                                                                                                                                                                
                            <Input value="{&nbsp;                                                                                                                                                                                                              
                                path: 'BankData>Amount',                                                                                                                                                                                                       
                                type: 'sap.ui.model.type.Currency'}"                                                                                                                                                                                           
                            />&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                 
                        </cells>                                                                                                                                                                                                                               
                        <cells>                                                                                                                                                                                                                                
                            <Text text="{ path: 'BankData>PaymentDate' }"/>&nbsp;                                                                                                                                                                              
                        </cells>                                                                                                                                                                                                                               
                        <cells>                                                                                                                                                                                                                                
                            <ComboBox&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                          
                                items="{                                                                                                                                                                                                                       
                                    path:'BankAccountData>/results/',                                                                                                                                                                                          
                                        sorter: {                                                                                                                                                                                                              
                                            path: 'BankAccountData>Bank',                                                                                                                                                                                      
                                            descending: false,                                                                                                                                                                                                 
                                            group: true                                                                                                                                                                                                        
                                        }                                                                                                                                                                                                                      
                                }" showSecondaryValues="true" selectionChange="onPressBankData">                                                                                                                                                               
                                <c:ListItem                                                                                                                                                                                                                    
                                    text="{BankAccountData>BankAgency}"                                                                                                                                                                                        
                                    additionalText="{BankAccountData>BankAccount}"                                                                                                                                                                             
                                    key="{BankAccountData>BankId}"                                                                                                                                                                                             
                                />                                                                                                                                                                                                                             
                            </ComboBox>                                                                                                                                                                                                                        
                        </cells>                                                                                                                                                                                                                               
                        <cells>                                                                                                                                                                                                                                
                            <ObjectIdentifier text="{BankData>Bank}"/>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbs+
p;&nbsp;                                                                                                                                                                                                                                                       
                        </cells>                                                                                                                                                                                                                               
                        <cells>                                                                                                                                                                                                                                
                            <ObjectIdentifier text="{BankData>BankCity}"/>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;+
&nbsp;&nbsp;                                                                                                                                                                                                                                                   
                        </cells>                                                                                                                                                                                                                               
                        <cells>                                                                                                                                                                                                                                
                            <ObjectIdentifier text="{BankData>BankAgency}"/>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbs+
p;&nbsp;&nbsp;                                                                                                                                                                                                                                                 
                        </cells>                                                                                                                                                                                                                               
                        <cells>                                                                                                                                                                                                                                
                            <ObjectIdentifier text="{BankData>BankAccount}"/>                                                                                                                                                                                  
                        </cells>                                                                                                                                                                                                                               
                        <cells>                                                                                                                                                                                                                                
                            <ObjectIdentifier text="{BankData>BankHolder}"/>                                                                                                                                                                                   
                        </cells>                                                                                                                                                                                                                               
                    </ColumnListItem>                                                                                                                                                                                                                          
                </items>                                                                                                                                                                                                                                       
            </Table>                                                                                                                                                                                                                                           
        </VBox>                                                                                                                                                                                                                                                
        <MessageStrip                                                                                                                                                                                                                                          
            id="MessageStripBank"                                                                                                                                                                                                                              
            visible="false"                                                                                                                                                                                                                                    
            class="sapUiSmallMarginBeginEnd sapUiSmallMarginTopBottom"                                                                                                                                                                                         
            text="Information with custom icon"                                                                                                                                                                                                                
            customIcon="sap-icon://warning"                                                                                                                                                                                                                    
            showIcon="true">                                                                                                                                                                                                                                   
        </MessageStrip>                                                                                                                                                                                                                                        
    </Dialog>                                                                                                                                                                                                                                                  
</c:FragmentDefinition>                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               