<mvc:View                                                                                                                                                                                                                                                      
	controllerName="royalties.zroyalties.controller.Detail"                                                                                                                                                                                                       
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:semantic="sap.m.semantic"                                                                                                                                                                                                                               
	xmlns:mvc="sap.ui.core.mvc"                                                                                                                                                                                                                                   
>                                                                                                                                                                                                                                                              
	<!-- <semantic:SemanticPage                                                                                                                                                                                                                                   
		id="detailPage"                                                                                                                                                                                                                                              
		busy="{detailView>/busy}"                                                                                                                                                                                                                                    
		busyIndicatorDelay="{detailView>/delay}"&nbsp;                                                                                                                                                                                                               
	> -->                                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
    <semantic:FullscreenPage                                                                                                                                                                                                                                   
		id="page0"&nbsp;                                                                                                                                                                                                                                             
		class="noScroll"                                                                                                                                                                                                                                             
		busy="{ProdProcView>/busy}"                                                                                                                                                                                                                                  
		enableScrolling="false"                                                                                                                                                                                                                                      
        title="{i18n>detailTitle}"                                                                                                                                                                                                                             
		showFooter="false"                                                                                                                                                                                                                                           
		customHeaderContent="true"                                                                                                                                                                                                                                   
	>&nbsp;                                                                                                                                                                                                                                                       
&nbsp;                                                                                                                                                                                                                                                         
        <Title                                                                                                                                                                                                                                                 
            id="listPageTitle"                                                                                                                                                                                                                                 
            text="{listView>/title}"                                                                                                                                                                                                                           
            level="H2"                                                                                                                                                                                                                                         
        />&nbsp;                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
		<semantic:customHeaderContent>                                                                                                                                                                                                                               
			<Button                                                                                                                                                                                                                                                     
				activeIcon=""                                                                                                                                                                                                                                              
				ariaHasPopup="None"                                                                                                                                                                                                                                        
				enabled="true"                                                                                                                                                                                                                                             
				icon="sap-icon://decline"                                                                                                                                                                                                                                  
				iconDensityAware="true"                                                                                                                                                                                                                                    
				iconFirst="true"                                                                                                                                                                                                                                           
				text=""                                                                                                                                                                                                                                                    
				textDirection="Inherit"                                                                                                                                                                                                                                    
				type="Default"                                                                                                                                                                                                                                             
				width=""                                                                                                                                                                                                                                                   
				press="onCloseDetailPress"                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
			>                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		  </Button>&nbsp;                                                                                                                                                                                                                                            
		</semantic:customHeaderContent>                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
		<semantic:content>                                                                                                                                                                                                                                           
			<smartTable:SmartTable                                                                                                                                                                                                                                      
				xmlns:smartTable="sap.ui.comp.smarttable"                                                                                                                                                                                                                  
				xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"                                                                                                                                                                               
				entitySet="ZADOC_ROYALTIES_LOG"                                                                                                                                                                                                                            
				smartFilterId="sfb_production_processing"                                                                                                                                                                                                                  
				tableType="Table"                                                                                                                                                                                                                                          
				header="{i18n>items}"                                                                                                                                                                                                                                      
				persistencyKey="SmartTableAnalytical_Explored"                                                                                                                                                                                                             
				class="sapUiResponsiveContentPadding"                                                                                                                                                                                                                      
				app:useSmartToggle="true"                                                                                                                                                                                                                                  
				initiallyVisibleFields="Discharge,CreatedOn,FiscalYear,DischargeStatus"                                                                                                                                                                                    
				enableAutoBinding="true"                                                                                                                                                                                                                                   
				showFullScreenButton="false"                                                                                                                                                                                                                               
				tableBindingPath="to_RoyaltiesLog"                                                                                                                                                                                                                         
				requestAtLeastFields="EdcNumber"                                                                                                                                                                                                                           
				id="st_log"                                                                                                                                                                                                                                                
				                                                                                                                                                                                                                                                           
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                               
			>                                                                                                                                                                                                                                                           
				<Table                                                                                                                                                                                                                                                     
					id="list3"                                                                                                                                                                                                                                                
					includeItemInSelection="true"                                                                                                                                                                                                                             
					contextualWidth="auto"                                                                                                                                                                                                                                    
					autoPopinMode="true"                                                                                                                                                                                                                                      
					sticky="ColumnHeaders"                                                                                                                                                                                                                                    
					busyIndicatorDelay="{listView>/delay}"                                                                                                                                                                                                                    
					noDataText="{listView>/noDataText}"                                                                                                                                                                                                                       
					mode="SingleSelectMaster"                                                                                                                                                                                                                                 
					growing="true"                                                                                                                                                                                                                                            
					growingScrollToLoad="true"                                                                                                                                                                                                                                
					                                                                                                                                                                                                                                                          
				/>                                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
				<smartTable:customToolbar>                                                                                                                                                                                                                                 
									<OverflowToolbar design="Transparent">                                                                                                                                                                                                                
										<ToolbarSpacer/>&nbsp;                                                                                                                                                                                                                               
										                                                                                                                                                                                                                                                     
										<OverflowToolbarButton                                                                                                                                                                                                                               
										icon="sap-icon://arrow-bottom"                                                                                                                                                                                                                       
										text="{i18n>discharge}"&nbsp;                                                                                                                                                                                                                        
										tooltip="{i18n>discharge}"                                                                                                                                                                                                                           
										press="onDischarge"&nbsp;                                                                                                                                                                                                                            
										/>                                                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
										<OverflowToolbarButton                                                                                                                                                                                                                               
											icon="sap-icon://delete"                                                                                                                                                                                                                            
											text="{i18n>delete}"                                                                                                                                                                                                                                
											tooltip="{i18n>delete}"                                                                                                                                                                                                                             
											press="onDischargeDelete"                                                                                                                                                                                                                           
										/>                                                                                                                                                                                                                                                   
									                                                                                                                                                                                                                                                      
									</OverflowToolbar>                                                                                                                                                                                                                                    
								</smartTable:customToolbar>                                                                                                                                                                                                                            
			</smartTable:SmartTable>                                                                                                                                                                                                                                    
		</semantic:content>                                                                                                                                                                                                                                          
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                               
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
	</semantic:FullscreenPage>                                                                                                                                                                                                                                    
</mvc:View>                                                                                                                                                                                                                                                    