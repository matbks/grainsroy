<mvc:View                                                                                                                                                                                                                                                      
	controllerName="royalties.zroyalties.controller.List"                                                                                                                                                                                                         
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:semantic="sap.m.semantic"                                                                                                                                                                                                                               
	xmlns:mvc="sap.ui.core.mvc"                                                                                                                                                                                                                                   
	id="monitorView"                                                                                                                                                                                                                                              
>                                                                                                                                                                                                                                                              
	<semantic:FullscreenPage                                                                                                                                                                                                                                      
		id="page02"                                                                                                                                                                                                                                                  
		busy="{ProdProcView>/busy}"                                                                                                                                                                                                                                  
		title="{i18n>mainTitle}"                                                                                                                                                                                                                                     
		showFooter="false"                                                                                                                                                                                                                                           
	>                                                                                                                                                                                                                                                             
		<semantic:content>                                                                                                                                                                                                                                           
			<smartFilterBar:SmartFilterBar                                                                                                                                                                                                                              
				xmlns:smartFilterBar="sap.ui.comp.smartfilterbar"                                                                                                                                                                                                          
				id="sfb_monitor"                                                                                                                                                                                                                                           
				entitySet="ZADOC_ROYALTIES_MONITOR"                                                                                                                                                                                                                        
				persistencyKey="SmartFilter_Explored"                                                                                                                                                                                                                      
				liveMode="true"&nbsp;&nbsp;                                                                                                                                                                                                                                
			>                                                                                                                                                                                                                                                           
				<smartFilterBar:controlConfiguration>                                                                                                                                                                                                                      
					<smartFilterBar:ControlConfiguration                                                                                                                                                                                                                      
						key="Property"                                                                                                                                                                                                                                           
						visibleInAdvancedArea="true"                                                                                                                                                                                                                             
						id="filterPlant2324"                                                                                                                                                                                                                                     
					/>                                                                                                                                                                                                                                                        
				</smartFilterBar:controlConfiguration>                                                                                                                                                                                                                     
				<smartFilterBar:controlConfiguration>                                                                                                                                                                                                                      
					<smartFilterBar:ControlConfiguration                                                                                                                                                                                                                      
						key="ApplicationDocNum"                                                                                                                                                                                                                                  
						visibleInAdvancedArea="true"                                                                                                                                                                                                                             
						id="productionOrderSFB2"                                                                                                                                                                                                                                 
					/>                                                                                                                                                                                                                                                        
				</smartFilterBar:controlConfiguration>                                                                                                                                                                                                                     
				<smartFilterBar:controlConfiguration>                                                                                                                                                                                                                      
					<smartFilterBar:ControlConfiguration                                                                                                                                                                                                                      
						key="CreatedBy"                                                                                                                                                                                                                                          
						visibleInAdvancedArea="true"                                                                                                                                                                                                                             
						id="filterProduct2232"                                                                                                                                                                                                                                   
					/>                                                                                                                                                                                                                                                        
				</smartFilterBar:controlConfiguration>                                                                                                                                                                                                                     
				<smartFilterBar:controlConfiguration>                                                                                                                                                                                                                      
					<smartFilterBar:ControlConfiguration                                                                                                                                                                                                                      
						key="EdcNum"                                                                                                                                                                                                                                             
						visibleInAdvancedArea="true"                                                                                                                                                                                                                             
						id="filterProduct33324"                                                                                                                                                                                                                                  
					/>                                                                                                                                                                                                                                                        
				</smartFilterBar:controlConfiguration>                                                                                                                                                                                                                     
				<smartFilterBar:controlConfiguration>                                                                                                                                                                                                                      
					<smartFilterBar:ControlConfiguration                                                                                                                                                                                                                      
						key="ContractNum"                                                                                                                                                                                                                                        
						visibleInAdvancedArea="true"                                                                                                                                                                                                                             
						id="filterProduct32343"                                                                                                                                                                                                                                  
					/>                                                                                                                                                                                                                                                        
				</smartFilterBar:controlConfiguration>                                                                                                                                                                                                                     
				<smartFilterBar:controlConfiguration>                                                                                                                                                                                                                      
					<smartFilterBar:ControlConfiguration                                                                                                                                                                                                                      
						key="ApplicationDocNum"                                                                                                                                                                                                                                  
						visibleInAdvancedArea="true"                                                                                                                                                                                                                             
						id="filterProduct34235"                                                                                                                                                                                                                                  
						label="{i18n>peneira}"                                                                                                                                                                                                                                   
					/>                                                                                                                                                                                                                                                        
				</smartFilterBar:controlConfiguration>                                                                                                                                                                                                                     
				<smartFilterBar:controlConfiguration>                                                                                                                                                                                                                      
					<smartFilterBar:ControlConfiguration                                                                                                                                                                                                                      
						key="CreatedOn"                                                                                                                                                                                                                                          
						visibleInAdvancedArea="true"                                                                                                                                                                                                                             
						id="filterProduct34323"                                                                                                                                                                                                                                  
					/>                                                                                                                                                                                                                                                        
				</smartFilterBar:controlConfiguration>                                                                                                                                                                                                                     
				<smartFilterBar:controlConfiguration>                                                                                                                                                                                                                      
					<smartFilterBar:ControlConfiguration                                                                                                                                                                                                                      
						key="Partner"                                                                                                                                                                                                                                            
						visibleInAdvancedArea="true"                                                                                                                                                                                                                             
						id="filterProduc2t3353"                                                                                                                                                                                                                                  
					/>                                                                                                                                                                                                                                                        
				</smartFilterBar:controlConfiguration>                                                                                                                                                                                                                     
			</smartFilterBar:SmartFilterBar>                                                                                                                                                                                                                            
			<smartTable:SmartTable                                                                                                                                                                                                                                      
				id="st_monitor"                                                                                                                                                                                                                                            
				xmlns:smartTable="sap.ui.comp.smarttable"                                                                                                                                                                                                                  
				xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"                                                                                                                                                                               
				entitySet="ZADOC_ROYALTIES_MONITOR"                                                                                                                                                                                                                        
				tableType="Table"                                                                                                                                                                                                                                          
				header="{i18n>items}"                                                                                                                                                                                                                                      
				persistencyKey="SmartTableAnalytical_Explored"                                                                                                                                                                                                             
				class="sapUiResponsiveContentPadding"                                                                                                                                                                                                                      
				app:useSmartToggle="true"                                                                                                                                                                                                                                  
				initiallyVisibleFields="EdcNum,ContractNum,Material,MaterialDescription,Property,PropertyDescription,ApplicationQuantity,Discharges,ChangedBy,ChangedOn,Romaneio,TicketDate,Partner,PartnerDescription,PartnerId,Plant"                                    
				showFullScreenButton="true"                                                                                                                                                                                                                                
				busyIndicatorDelay="{listView>/delay}"                                                                                                                                                                                                                     
				smartFilterId="sfb_monitor"                                                                                                                                                                                                                                
				enableAutoBinding="false"                                                                                                                                                                                                                                  
			>                                                                                                                                                                                                                                                           
				<Table                                                                                                                                                                                                                                                     
					id="list"                                                                                                                                                                                                                                                 
					includeItemInSelection="true"                                                                                                                                                                                                                             
					contextualWidth="auto"                                                                                                                                                                                                                                    
					autoPopinMode="true"                                                                                                                                                                                                                                      
					sticky="ColumnHeaders,HeaderToolbar,InfoToolbar"                                                                                                                                                                                                          
					busyIndicatorDelay="{listView>/delay}"                                                                                                                                                                                                                    
					noDataText="{listView>/noDataText}"                                                                                                                                                                                                                       
					mode="SingleSelectMaster"                                                                                                                                                                                                                                 
					growing="true"                                                                                                                                                                                                                                            
					growingScrollToLoad="true"                                                                                                                                                                                                                                
					growingThreshold="30"                                                                                                                                                                                                                                     
					updateFinished=".onUpdateFinished"                                                                                                                                                                                                                        
					selectionChange=".onSelectionChange"                                                                                                                                                                                                                      
				/>                                                                                                                                                                                                                                                         
				<smartTable:customToolbar>                                                                                                                                                                                                                                 
					<OverflowToolbar                                                                                                                                                                                                                                          
						design="Transparent"                                                                                                                                                                                                                                     
						id="toolBar"                                                                                                                                                                                                                                             
					>                                                                                                                                                                                                                                                         
						<ToolbarSpacer id="toolBaarSpacer"/>                                                                                                                                                                                                                     
						<OverflowToolbarButton                                                                                                                                                                                                                                   
							id="reportIcon"                                                                                                                                                                                                                                         
							icon="sap-icon://manager-insight"                                                                                                                                                                                                                       
							text="{i18n>report}"                                                                                                                                                                                                                                    
							tooltip="{i18n>report}"                                                                                                                                                                                                                                 
							press="onShowReport"                                                                                                                                                                                                                                    
						/>                                                                                                                                                                                                                                                       
						<OverflowToolbarButton                                                                                                                                                                                                                                   
							id="dischargeIcon"                                                                                                                                                                                                                                      
							icon="sap-icon://drill-down"                                                                                                                                                                                                                            
							text="{i18n>discharge}"                                                                                                                                                                                                                                 
							tooltip="{i18n>discharge}"                                                                                                                                                                                                                              
							press="onMassDischarge"                                                                                                                                                                                                                                 
						/>                                                                                                                                                                                                                                                       
					</OverflowToolbar>                                                                                                                                                                                                                                        
				</smartTable:customToolbar>                                                                                                                                                                                                                                
			</smartTable:SmartTable>                                                                                                                                                                                                                                    
		</semantic:content>                                                                                                                                                                                                                                          
		<semantic:messagesIndicator>                                                                                                                                                                                                                                 
			<semantic:MessagesIndicator                                                                                                                                                                                                                                 
				id="MessageIndicator"                                                                                                                                                                                                                                      
				press="onMessagesButtonPress"                                                                                                                                                                                                                              
			/>                                                                                                                                                                                                                                                          
		</semantic:messagesIndicator>                                                                                                                                                                                                                                
		<!-- <semantic:customFooterContent>                                                                                                                                                                                                                          
			<Button                                                                                                                                                                                                                                                     
				id="ButtonSolicitation"                                                                                                                                                                                                                                    
				icon="sap-icon://arrow-bottom"                                                                                                                                                                                                                             
				text="{i18n>discharge}"                                                                                                                                                                                                                                    
				type="Ghost"                                                                                                                                                                                                                                               
				press="onDischarge"                                                                                                                                                                                                                                        
			/>                                                                                                                                                                                                                                                          
		</semantic:customFooterContent> -->                                                                                                                                                                                                                          
	</semantic:FullscreenPage>                                                                                                                                                                                                                                    
</mvc:View>                                                                                                                                                                                                                                                    