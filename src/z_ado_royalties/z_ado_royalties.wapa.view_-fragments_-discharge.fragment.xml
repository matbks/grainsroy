<c:FragmentDefinition                                                                                                                                                                                                                                          
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:c="sap.ui.core"                                                                                                                                                                                                                                         
	xmlns:f="sap.ui.layout.form"                                                                                                                                                                                                                                  
>                                                                                                                                                                                                                                                              
	<Dialog                                                                                                                                                                                                                                                       
		id="openDialog"                                                                                                                                                                                                                                              
		title="{i18n>discharge}"                                                                                                                                                                                                                                     
	>                                                                                                                                                                                                                                                             
		<beginButton>                                                                                                                                                                                                                                                
			<Button                                                                                                                                                                                                                                                     
				text="{i18n>save}"                                                                                                                                                                                                                                         
				type="Accept"                                                                                                                                                                                                                                              
				press="handleSaveBtnPress"                                                                                                                                                                                                                                 
			/>                                                                                                                                                                                                                                                          
		</beginButton>                                                                                                                                                                                                                                               
		<endButton>                                                                                                                                                                                                                                                  
			<Button                                                                                                                                                                                                                                                     
				text="{i18n>cancel}"                                                                                                                                                                                                                                       
				type="Reject"                                                                                                                                                                                                                                              
				press="handleCancelBtnPress"                                                                                                                                                                                                                               
			/>                                                                                                                                                                                                                                                          
		</endButton>                                                                                                                                                                                                                                                 
		<f:SimpleForm                                                                                                                                                                                                                                                
			id="RegisterForm"                                                                                                                                                                                                                                           
			editable="true"                                                                                                                                                                                                                                             
		>                                                                                                                                                                                                                                                            
			<f:content>                                                                                                                                                                                                                                                 
				<!-- <Label text="{/#Customers/Cpf/@sap:label}"/>                                                                                                                                                                                                          
				<MaskInput                                                                                                                                                                                                                                                 
					value="{Customer>/Cpf}"                                                                                                                                                                                                                                   
					mask="999.999.999-99"                                                                                                                                                                                                                                     
					placeholder="{i18n>addCPF}"                                                                                                                                                                                                                               
				/> -->                                                                                                                                                                                                                                                     
				<Label text="Propriedade"/>                                                                                                                                                                                                                                
				<Input value="{Monitor>/Property}" editable="false" enabled="false"/>                                                                                                                                                                                      
                                                                                                                                                                                                                                                               
				<Label text="Protocolo"/>                                                                                                                                                                                                                                  
				<Input id="Protocol" />                                                                                                                                                                                                                                    
				&nbsp;                                                                                                                                                                                                                                                     
				<Label text="{i18n>BalanceAmount}"/>                                                                                                                                                                                                                       
				<Input&nbsp;                                                                                                                                                                                                                                               
					editable="false"&nbsp;                                                                                                                                                                                                                                    
					enabled="false"                                                                                                                                                                                                                                           
					value="{Monitor>/Balance}"                                                                                                                                                                                                                                
					placeholder="{i18n>TypeBalanceAmount}"                                                                                                                                                                                                                    
					id="balanceInput"                                                                                                                                                                                                                                         
				/>                                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
				<Label text="{i18n>DischargeAmount}"/>                                                                                                                                                                                                                     
					<Input&nbsp;                                                                                                                                                                                                                                              
						placeholder="{i18n>TypeDischargeAmount}"                                                                                                                                                                                                                 
						id="dischargeInput"                                                                                                                                                                                                                                      
						required="true"                                                                                                                                                                                                                                          
						type="Number"                                                                                                                                                                                                                                            
					/>                                                                                                                                                                                                                                                        
			</f:content>                                                                                                                                                                                                                                                
		</f:SimpleForm>                                                                                                                                                                                                                                              
	</Dialog>                                                                                                                                                                                                                                                     
</c:FragmentDefinition>                                                                                                                                                                                                                                        